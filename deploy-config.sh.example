#!/bin/bash
# 배포 설정 파일 예제
# 이 파일을 deploy-config.sh로 복사하고 실제 값을 입력하세요

# 서버 접속 정보
export SERVER_USER="your_username"
export SERVER_HOST="your_server_ip"
export SERVER_PORT="22"
export SSH_KEY="~/.ssh/id_ed25519"
export REMOTE_DIR="/home/your_username/my-platform"

# 서버 URL (선택사항 - 배포 완료 메시지에 표시)
export SERVER_URL="http://your-domain.com"